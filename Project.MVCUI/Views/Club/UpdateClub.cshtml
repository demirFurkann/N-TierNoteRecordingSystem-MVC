@model ClubAddUpdatePageVM
@{
    ViewBag.Title = "UpdateClub";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.HiddenFor(x=> x.Club.ID)
    <div class="form-group">

        <div class="col-sm-10">
            <label class="row-cols-4">Kulüp Adı</label>
            @Html.TextBoxFor(x => x.Club.ClubName, new { @class = "form-control" })
        </div>
    </div>
    <br />
    <div class="form-group">

        <div class="col-sm-10">
            <label class="row-cols-4">Kontejyan</label>
            @Html.TextBoxFor(x => x.Club.Quato, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">

        <div class="col-sm-10">
            <button class="btn-primary">Ekle</button>
        </div>
    </div>



}

